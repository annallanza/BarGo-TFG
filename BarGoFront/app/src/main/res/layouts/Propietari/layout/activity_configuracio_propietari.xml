<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UsuariPropietari.Activity.ConfiguracioPropietariActivity">

    <TextView
        android:id="@+id/textViewConfiguracio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:autoSizeTextType="uniform"
        android:maxLines="1"
        android:text="@string/configuraci_n"
        android:textColor="#323131"
        android:textSize="36sp"
        android:textStyle="bold"
        app:autoSizeMaxTextSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/textViewLogout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/quieres_cerrar_la_sesi_n"
        android:textColor="#000000"
        android:textSize="18sp"
        app:autoSizeMaxTextSize="18sp"
        android:autoSizeTextType="uniform"
        android:maxLines="1"
        tools:targetApi="o"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textViewConfiguracio" />

    <TextView
        android:id="@+id/textViewEliminarCuenta2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/quieres_eliminar_tu_cuenta"
        android:textColor="#000000"
        android:textSize="18sp"
        app:autoSizeMaxTextSize="18sp"
        android:autoSizeTextType="uniform"
        android:maxLines="1"
        tools:targetApi="o"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewLogout2" />
</androidx.constraintlayout.widget.ConstraintLayout>